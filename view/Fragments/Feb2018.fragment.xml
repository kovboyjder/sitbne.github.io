<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:uxap="sap.uxap">
	<uxap:ObjectPageLayout id="Feb2018PageLayout">
		<uxap:headerTitle>
			<uxap:ObjectPageHeader objectTitle="{i18n>feb2018Title}" objectSubtitle="{i18n>feb2018Subtitle}"/>
		</uxap:headerTitle>
		<uxap:sections>
			<uxap:ObjectPageSection title="{i18n>feb2018WorkflowSection}">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>feb2018WorkflowSubSection}">
						<uxap:blocks>
							<Text text="{i18n>feb2018WorkflowText}"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>

			<uxap:ObjectPageSection title="{i18n>feb2018GatewaySection}">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>feb2018GatewaySubSection}">
						<uxap:blocks>
							<Text text="{i18n>feb2018GatewayText}"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
					<uxap:ObjectPageSubSection title="{i18n>feb2018GatewayExtraSubSection}">
						<uxap:blocks>
							<Text text="{i18n>feb2018GatewayExtraText}"/>
							<HBox>
								<VBox>
									<Link text="Slide Content" href="/media/ODATA Performance Handling.pptx" target="_blank" />
			
								</VBox>
							</HBox>
									
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>

			<uxap:ObjectPageSection title="{i18n>feb2018AbapGitSection}">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>feb2018AbapGitSubSection}">
						<uxap:blocks>
							<Text text="{i18n>feb2018AbapGitText}"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
					<uxap:ObjectPageSubSection title="{i18n>feb2018AbapGitExtraSubSection}">
						<uxap:blocks>
							<Text text="{i18n>feb2018AbapGitExtraText}"/>
							<HBox>
								<VBox>
									<Link text="Video Content" href="/media/ABAPGit.zip" target="_blank" />
			
								</VBox>
							</HBox>
									
						</uxap:blocks>
					</uxap:ObjectPageSubSection>

				</uxap:subSections>
			</uxap:ObjectPageSection>

		</uxap:sections>
	</uxap:ObjectPageLayout>
</core:FragmentDefinition>